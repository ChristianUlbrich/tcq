{
  "name": "tcq",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/bterlson/tcq"
  },
  "description": "A TC39 Discussion Queue",
  "main": "index.js",
  "scripts": {
    "build-frontend": "webpack --config ./src/client/webpack.dev.js",
    "build-frontend-watch": "webpack --watch --config ./src/client/webpack.dev.js",
    "build-backend": "tsc -p ./src/server",
    "build-production": "npm run build-backend && webpack -p --config ./src/client/webpack.prod.js",
    "iterate": "npm run build-backend && concurrently \"npm run build-frontend-watch\" \"node ./dist/server/server.js\"",
    "start": "node dist/server/server.js | \"./node_modules/.bin/bunyan\""
  },
  "author": "Brian Terlson",
  "contributors": [
    {
      "name": "Daniel Rosenwasser",
      "url": "https://github.com/DanielRosenwasser"
    }
  ],
  "license": "Apache-2.0",
  "engines": {
    "node": "8.0.0"
  },
  "dependencies": {
    "applicationinsights": "^1.7.3",
    "axios": "^0.19.2",
    "base64-url": "^2.3.3",
    "body-parser": "^1.19.0",
    "bulma": "^0.6.2",
    "bunyan": "^1.8.12",
    "connect-ensure-login": "^0.1.1",
    "cookie-session": "^1.4.0",
    "documentdb": "^1.15.3",
    "documentdb-session": "^1.0.1",
    "documentdb-typescript": "^1.0.7",
    "express": "^4.17.1",
    "express-bunyan-logger": "^1.3.3",
    "express-session": "^1.17.0",
    "github": "^14.0.0",
    "p-limit": "^1.3.0",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "uuid": "^3.4.0",
    "vue": "^2.6.11",
    "vuedraggable": "^2.23.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.0",
    "@types/bunyan": "^1.8.6",
    "@types/connect-ensure-login": "^0.1.5",
    "@types/documentdb": "^1.10.5",
    "@types/documentdb-session": "^1.0.1",
    "@types/express": "^4.17.3",
    "@types/express-session": "^1.17.0",
    "@types/node": "^13.9.0",
    "@types/passport": "^1.0.2",
    "@types/passport-github": "^1.1.5",
    "@types/socket.io": "^1.4.40",
    "@types/socket.io-client": "^1.4.32",
    "@types/uuid": "^3.4.8",
    "concurrently": "^3.6.1",
    "css-loader": "^3.4.2",
    "extract-text-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.13.1",
    "sass-loader": "^6.0.7",
    "strict-event-emitter-types": "^1.2.0",
    "style-loader": "^0.19.1",
    "ts-loader": "^2.3.7",
    "typescript": "^3.8.3",
    "vue-template-compiler": "^2.6.11",
    "vue-template-loader": "^0.3.1",
    "webpack": "^4.42.0",
    "webpack-merge": "^4.2.2"
  }
}
