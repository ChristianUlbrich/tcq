{
  "name": "tcq",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/bterlson/tcq"
  },
  "description": "A TC39 Discussion Queue",
  "main": "dist/server/server.js",
  "scripts": {
    "build-frontend": "webpack --config ./src/client/webpack.dev.js",
    "build-frontend-watch": "webpack --watch --config ./src/client/webpack.dev.js",
    "build-backend": "tsc -p ./src/server",
    "build-production": "npm run build-backend && webpack -p --config ./src/client/webpack.prod.js",
    "iterate": "npm run build-backend && concurrently \"npm run build-frontend-watch\" \"node ./dist/server/server.js\"",
    "start": "node -r dotenv/config dist/server/server.js | \"./node_modules/.bin/bunyan\""
  },
  "author": "Brian Terlson",
  "contributors": [
    {
      "name": "Daniel Rosenwasser",
      "url": "https://github.com/DanielRosenwasser"
    }
  ],
  "license": "Apache-2.0",
  "engines": {
    "node": "^20.9.0"
  },
  "type": "module",
  "dependencies": {
    "@azure/cosmos": "4.0.0",
    "axios": "1.6.1",
    "base64-url": "2.3.3",
    "body-parser": "1.20.2",
    "bulma": "0.9.4",
    "bunyan": "1.8.15",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "express-bunyan-logger": "1.3.3",
    "express-session": "1.17.3",
    "github": "^13.1.1",
    "p-limit": "^1.2.0",
    "passport": "0.6.0",
    "passport-github2": "0.1.12",
    "socket.io": "4.7.2",
    "socket.io-client": "^2.0.3",
    "uuid": "9.0.1",
    "vue": "^2.5.2",
    "vuedraggable": "^2.14.1"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/body-parser": "1.19.5",
    "@types/bunyan": "1.8.11",
    "@types/express": "4.17.21",
    "@types/express-bunyan-logger": "1.3.6",
    "@types/express-session": "1.17.10",
    "@types/node": "20.9.0",
    "@types/passport": "1.0.15",
    "@types/socket.io": "3.0.2",
    "@types/socket.io-client": "^1.4.31",
    "@types/uuid": "9.0.7",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "^2.30.1",
    "sass": "^1.3.0",
    "sass-loader": "^7.0.0",
    "strict-event-emitter-types": "2.0.0",
    "style-loader": "^0.19.0",
    "ts-loader": "^2.3.7",
    "typescript": "5.2.2",
    "vue-template-compiler": "^2.5.2",
    "vue-template-loader": "^0.3.1",
    "webpack": "^3.6.0",
    "webpack-merge": "^4.1.0"
  }
}
