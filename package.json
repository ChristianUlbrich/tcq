{
  "name": "@tc39/tcq-reloaded",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/zalari/tcq"
  },
  "description": "A TC39 Discussion Queue",
  "main": "index.js",
  "scripts": {
    "build-frontend": "webpack --config ./src/client/webpack.dev.js",
    "build-frontend-watch": "webpack --watch --config ./src/client/webpack.dev.js",
    "build-backend": "tsc -p ./src/server",
    "build-production": "npm run build-backend && webpack -p --config ./src/client/webpack.prod.js",
    "iterate": "npm run build-backend && concurrently \"npm run build-frontend-watch\" \"node ./dist/server/server.js\"",
    "start": "node dist/server/server.js | \"./node_modules/.bin/bunyan\"",
    "docker:build:backend": "docker run --rm -v .:/opt/build tc39/tcq-builder npm run build-backend",
    "docker:build:frontend": "docker run --rm -v .:/opt/build tc39/tcq-builder npm run build-frontend",
    "docker:build:production": "docker run --rm -v .:/opt/build tc39/tcq-builder npm run build-production"
  },
  "author": "Brian Terlson",
  "contributors": [
    {
      "name": "Daniel Rosenwasser",
      "url": "https://github.com/DanielRosenwasser"
    }
  ],
  "license": "Apache-2.0",
  "engines": {
    "node": "10.19.0"
  },
  "dependencies": {
    "axios": "0.16.2",
    "base64-url": "2.0.0",
    "body-parser": "1.18.2",
    "bulma": "0.6.0",
    "bunyan": "1.8.12",
    "connect-ensure-login": "0.1.1",
    "cookie-session": "1.3.3",
    "documentdb": "1.13.0",
    "documentdb-session": "1.0.1",
    "documentdb-typescript": "1.0.6",
    "express": "4.16.2",
    "express-bunyan-logger": "1.3.2",
    "express-session": "1.15.6",
    "github": "11.0.0",
    "p-limit": "1.2.0",
    "passport": "0.4.0",
    "passport-github2": "0.1.12",
    "socket.io": "2.2.0",
    "socket.io-client": "2.0.4",
    "uuid": "3.1.0",
    "vue": "2.5.2",
    "vuedraggable": "2.14.1"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/body-parser": "1.16.7",
    "@types/bunyan": "1.8.3",
    "@types/connect-ensure-login": "0.1.3",
    "@types/documentdb": "1.10.3",
    "@types/documentdb-session": "1.0.1",
    "@types/express": "4.0.39",
    "@types/express-session": "1.15.5",
    "@types/node": "8.0.47",
    "@types/passport": "0.3.5",
    "@types/socket.io": "1.4.31",
    "@types/socket.io-client": "1.4.31",
    "@types/uuid": "3.4.3",
    "concurrently": "3.5.0",
    "css-loader": "0.28.7",
    "extract-text-webpack-plugin": "3.0.2",
    "html-webpack-plugin": "2.30.1",
    "node-sass": "4.11.0",
    "sass-loader": "6.0.6",
    "strict-event-emitter-types": "1.2.0",
    "style-loader": "0.19.0",
    "ts-loader": "2.3.7",
    "typescript": "2.8.1",
    "vue-template-compiler": "2.5.2",
    "vue-template-loader": "0.3.1",
    "webpack": "3.8.1",
    "webpack-merge": "4.1.0"
  }
}
